package com.object {		import flash.display.MovieClip;	import flash.events.Event;	import com.greensock.*;	import com.greensock.easing.*;		public class CookieMan extends MovieClip {				private var body:CookieBody;		private var acc:CookieAcc;		private var hand:CookieHand;		private var face:CookieFace;		public var unit:Number = new Number();						public function CookieMan() {			this.addEventListener(Event.ADDED_TO_STAGE,loadInit);					}						public function loadInit(e:Event):void		{						this.removeEventListener(Event.ADDED_TO_STAGE,loadInit);			body = new CookieBody();			acc = new CookieAcc();			hand = new CookieHand();			face = new CookieFace();			//			this.addChild(body);			this.addChild(acc);			this.addChild(hand);			this.addChild(face);			this.scaleX = this.scaleY = .01;			var wh = body.width/body.height;			var rate:Number = new Number();			var wid:Number;			var hei:Number;			var dtime:Number = 0;			var rate2:Number;			//dtime = Math.round(Math.random()*30)/100;			if(wh<1)			{				rate = body.height/unit;				hei = unit;				wid = body.width/rate;				rate2 = unit/body.height;				TweenLite.killTweensOf(this);				//TweenLite.to(this, 1, {width:wid,height:hei,ease:Elastic.easeOut,delay:dtime});				TweenLite.to(this, 1, {scaleX:rate2,scaleY:rate2,ease:Elastic.easeOut,delay:dtime});			}else{				rate = body.width/unit;				wid = unit;				hei= body.height/rate; 				rate2 = unit/body.width;				TweenLite.killTweensOf(this);				//TweenLite.to(this, 1, {width:wid,height:hei,ease:Elastic.easeOut,delay:dtime});				TweenLite.to(this, 1, {scaleX:rate2,scaleY:rate2,ease:Elastic.easeOut,delay:dtime});			}														}	}	}