package com.object {		import flash.display.MovieClip;	import flash.display.Loader;	import flash.net.URLRequest;	import flash.events.ProgressEvent;	import flash.events.Event;	import flash.display.LoaderInfo;	import fl.controls.CheckBox;	import flash.events.MouseEvent;			public class OneFriendData extends MovieClip {				private var _fb_id:String = new String();		private var _fb_name:String = new String();		public var myCheckBox:CheckBox = new CheckBox();				             		function loadImage(clip:String, holder:MovieClip):void		{			//removeChildren(holder);			 			var imageLoader:Loader = new Loader();			var imageURL:String = clip			var imageURLReq:URLRequest = new URLRequest(imageURL);						holder.addChild(imageLoader);			trace("loading "+imageURL);			 			imageLoader.load(imageURLReq);			imageLoader.contentLoaderInfo.addEventListener(Event.INIT, imageLoaded);			imageLoader.contentLoaderInfo.addEventListener(ProgressEvent.PROGRESS, imageProgress);		}		 		function imageProgress (evt:ProgressEvent):void		{		//trace(Math.round((evt.bytesLoaded / evt.bytesTotal)*100));		}		 		function imageLoaded(evt:Event):void		{		//var targetLoader:Loader = Loader(evt.target.loader);		//trace("complete");			//var mc = Loader(evt.currentTarget);									var loader : Loader = (evt.currentTarget as LoaderInfo).loader;			loader.parent.width = loader.parent.height = 75;		}		 		function removeChildrenW (container:MovieClip):void		{			while (container.numChildren)			{			container.removeChildAt (0);			}		}				private function set fb_id(str:String)		{			_fb_id = str;		}				private function set fb_name(str:String)		{			_fb_name = str;		}								public function OneFriendData() {			// constructor code		}						public function setData(fb_id,fb_name,pic_url)		{						this.fb_id = fb_id;			this.fb_name = fb_name;			fname_txt.text = fb_name;			loadImage(pic_url,photo);			addChild(myCheckBox);			myCheckBox.addEventListener(MouseEvent.CLICK, _selected);			myCheckBox.y = 30;			myCheckBox.label ="";		}										function _selected(e:MouseEvent):void		{			var bool:Boolean = e.target.selected;					if(bool)			{				//trace("was checked");				// more code				trace(this._fb_id +","+this._fb_name)				this.dispatchEvent(new MyEvent("SELECT_CHECKBOX",{fb_id:this._fb_id,fb_name:this._fb_name, ta:this.name}));			}			else			{
				this.dispatchEvent(new MyEvent("NO_SELECT_CHECKBOX",{fb_id:this._fb_id,fb_name:this._fb_name,ta:this.name}));				//trace("was unchecked");				// more code			}		}	}	}